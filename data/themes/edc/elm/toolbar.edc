            visible: 1;
         }
      }
      part { name: "elm.swallow.icon_new"; type: SWALLOW;
         clip_to: "icon_clip2";
         description { state: "default" 0.0;
            align: 0.5 0.5;
            rel1.relative: 0.0 0.0;
            rel1.offset: 2 2;
            rel2.to_y: "elm.text";
            rel2.relative: 1.0 0.0;
            rel2.offset: -3 -1;
         }
         description { state: "noicon" 0.0;

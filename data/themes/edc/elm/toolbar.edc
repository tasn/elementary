            rel2.to_y: "icon_clip";
         }
      }
      part { name: "icon_clip2"; type: RECT;
         clip_to: "icon_clip";
         description { state: "default" 0.0;
            color: 255 255 255 0;
            visible: 0;
         }
         description { state: "animation" 0.0;
            inherit: "default" 0.0;
